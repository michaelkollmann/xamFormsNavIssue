<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="navigationissue.Views.ModalPage"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <ContentPage.Content>
        <StackLayout>
            <Label
                HorizontalOptions="CenterAndExpand"
                Text="Modal dialog after child navigation"
                VerticalOptions="CenterAndExpand" />
            <Button Clicked="Button_Clicked" Text="Close modal" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>